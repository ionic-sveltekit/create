{
  "name": "@ionic-sveltekit/create",
  "version": "0.0.1",
  "description": "CLI tool to create a new SvelteKit project with Ionic UI components",
  "bugs": {
    "url": "https://github.com/ionic-sveltekit/create/issues"
  },
  "homepage": "https://github.com/ionic-sveltekit/create#readme",
  "keywords": [
    "Ionic",
    "SvelteKit",
    "ionic-sveltekit",
    "CLI",
    "create",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-sveltekit/create.git"
  },
  "license": "MIT",
  "author": "briznad@gmail.com",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "create-ionic-sveltekit": "./src/index.js"
  },
  "files": [
    "src/**"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "dev": "node src/index.js",
    "long": "node src/index.js --types=typescript --prettier --eslint --playwright=false -f svelte-kit --ionicons=false",
    "pub:beta": "npm publish --tag beta",
    "pub:next": "npm publish --tag next",
    "pub:release": "npm publish",
    "release": "npm publish",
    "start": "node dist/index.js",
    "github:npm_publish": "npm publish --provenance --access public"
  },
  "dependencies": {
    "create-svelte": "^6.3.5",
    "fs-extra": "^10.1.0",
    "ip": "^1.1.8",
    "kleur": "^4.1.5",
    "mri": "^1.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "eslint": "^8.28.0",
    "standard-version": "^9.5.0",
    "svelte": "^4.2.7"
  }
}
